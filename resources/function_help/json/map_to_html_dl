{
  "name": "map_to_html_dl",
  "type": "function",
  "groups": ["Maps"],
  "description": "Merge map elements into a HTML definition list string.",
  "arguments": [{
    "arg": "map",
    "description": "the input map"
  }],
  "examples": [{
    "expression": "map_to_html_dl(map('qgis','rocks'))",
    "returns": "<dl><dt>qgis</dt><dd>rocks</dd></dl>"
  }],
  "tags": ["formatted", "map", "html"]
}
